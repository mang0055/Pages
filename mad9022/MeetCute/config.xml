<?xml version='1.0' encoding='utf-8'?>
    <widget id="ca.edumedia.mang0055.meetcute" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
        <name>Mang0055 MeetCute</name>
        <description>
            Android Application Developer
        </description>
        <author email="mang0055@algonquinlive.com" href="http://iamraviraj.com">
            Raviraj Mangukiya
        </author>
        <content src="index.html" />
        <plugin name="cordova-plugin-whitelist" version="1" />
        <access origin="*" />
        <allow-intent href="http://*/*" />
        <allow-intent href="https://*/*" />
        <allow-intent href="tel:*" />
        <allow-intent href="sms:*" />
        <allow-intent href="mailto:*" />
        <allow-intent href="geo:*" />
        <preference name="DisallowOverscroll" value="true" />
        <preference name="webviewbounce" value="false" />
        <preference name="StatusBarOverlaysWebView" value="false" />
        <feature name="StatusBar">
            <param name="ios-package" value="CDVStatusBar" onload="true" />
        </feature>
        <preference name="SplashScreen" value="screen" />
        <preference name="FadeSplashScreen" value="true" />
        <platform name="android">
            <allow-intent href="market:*" />
            <!--
            ldpi    : 36x36 px
            mdpi    : 48x48 px
            hdpi    : 72x72 px
            xhdpi   : 96x96 px
            xxhdpi  : 144x144 px
            xxxhdpi : 192x192 px
            -->
            <icon src="www/res/icons/android/icon-36-ldpi.png" density="ldpi" />
            <icon src="www/res/icons/android/icon-48-mdpi.png" density="mdpi" />
            <icon src="www/res/icons/android/icon-72-hdpi.png" density="hdpi" />
            <icon src="www/res/icons/android/icon-96-xhdpi.png" density="xhdpi" />
            <icon src="www/res/icons/android/icon-144-xxhdpi.png" density="xxhdpi" />
            <icon src="www/res/icons/android/icon-192-xxxhdpi.png" density="xxxhdpi" />
            <splash src="www/res/screen/android/screen-hdpi-portrait.png" density="port-hdpi" />
            <splash src="www/res/screen/android/screen-ldpi-portrait.png" density="port-ldpi" />
            <splash src="www/res/screen/android/screen-mdpi-portrait.png" density="port-mdpi" />
            <splash src="www/res/screen/android/screen-xhdpi-portrait.png" density="port-xhdpi" />
        </platform>
        <platform name="ios">
            <allow-intent href="itms:*" />
            <allow-intent href="itms-apps:*" />
            <!-- iOS 8.0+ -->
            <!-- iPhone 6 Plus  -->
            <icon src="www/res/icons/ios/icon-60-3x.png" width="180" height="180" />
            <!-- iOS 7.0+ -->
            <!-- iPhone / iPod Touch  -->
            <icon src="www/res/icons/ios/icon-60.png" width="60" height="60" />
            <icon src="www/res/icons/ios/icon-60-2x.png" width="120" height="120" />
            <!-- iPad -->
            <icon src="www/res/icons/ios/icon-76.png" width="76" height="76" />
            <icon src="www/res/icons/ios/icon-76-2x.png" width="152" height="152" />
            <!-- iOS 6.1 -->
            <!-- Spotlight Icon -->
            <icon src="www/res/icons/ios/icon-40.png" width="40" height="40" />
            <icon src="www/res/icons/ios/icon-40-2x.png" width="80" height="80" />
            <!-- iPhone / iPod Touch -->
            <icon src="www/res/icons/ios/icon.png" width="57" height="57" />
            <icon src="www/res/icons/ios/icon-2x.png" width="114" height="114" />
            <!-- iPad -->
            <icon src="www/res/icons/ios/icon-72.png" width="72" height="72" />
            <icon src="www/res/icons/ios/icon-72-2x.png" width="144" height="144" />
            <!-- iPhone Spotlight and Settings Icon -->
            <icon src="www/res/icons/ios/icon-small.png" width="29" height="29" />
            <icon src="www/res/icons/ios/icon-small-2x.png" width="58" height="58" />
            <!-- iPad Spotlight and Settings Icon -->
            <icon src="www/res/icons/ios/icon-50.png" width="50" height="50" />
            <icon src="www/res/icons/ios/icon-50-2x.png" width="100" height="100" />

            <splash src="www/res/screen/ios/Default~iphone.png" width="320" height="480" />
            <splash src="www/res/screen/ios/Default@2x~iphone.png" width="640" height="960" />
            <splash src="www/res/screen/ios/Default-Portrait~ipad.png" width="768" height="1024" />
            <splash src="www/res/screen/ios/Default-Portrait@2x~ipad.png" width="1536" height="2048" />
            <splash src="www/res/screen/ios/Default-Landscape~ipad.png" width="1024" height="768" />
            <splash src="www/res/screen/ios/Default-Landscape@2x~ipad.png" width="2048" height="1536" />
            <splash src="www/res/screen/ios/Default-568h@2x~iphone.png" width="640" height="1136" />
            <splash src="www/res/screen/ios/Default-667h.png" width="750" height="1334" />
            <splash src="www/res/screen/ios/Default-736h.png" width="1242" height="2208" />
            <splash src="www/res/screen/ios/Default-Landscape-736h.png" width="2208" height="1242" />
        </platform>
    </widget>